<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <entity name="App\Entity\Hgv" table="hgv">
        <!--indexes>
            <index name="id_idx" columns="id"/>
        </indexes-->
        <!--unique-constraints>
            <unique-constraint columns="id" name="uuid_idx" />
        </unique-constraints-->
        <!--lifecycle-callbacks>
            <lifecycle-callback type="prePersist" method="doStuffOnPrePersist"/>
            <lifecycle-callback type="prePersist" method="doOtherStuffOnPrePersistToo"/>
            <lifecycle-callback type="postPersist" method="doStuffOnPostPersist"/>
        </lifecycle-callbacks-->

        <!--id name="id" type="uuid"/-->
        <id name="id" type="string" length="64"><!-- texIdLang (TmNr + texLett + mehrfachKennung) -->
            <!-- unique -->
            <!-- index -->
            <!--generator strategy="AUTO"/>
            <sequence-generator sequence-name="tablename_seq" allocation-size="100" initial-value="1" /-->
        </id>
        <field name="tmNr" column="tmNr" type="integer" nullable="true" />
        <field name="texLett" column="texLett" type="text" nullable="true" />
        <field name="mehrfachKennung" column="mehrfachKennung" type="text" nullable="true" />
        <field name="hgvId" column="hgvId" type="text" nullable="true" />
        <field name="publikation" type="text" nullable="true" />
        <field name="band" type="integer" nullable="true" />
        <field name="zusBand" column="zusBand" type="text" nullable="true" />
        <field name="nummer" column="nummer" type="text" nullable="true" />
        <field name="seite" type="text" nullable="true" />
        <field name="zusaetzlich" type="text" nullable="true" />
        <field name="publikationLang" column="publikationLang" type="text" nullable="true" />
        <field name="material" type="text" nullable="true" />
        <field name="zusaetzlichSort" column="zusaetzlichSort" type="text" nullable="true" />
        <field name="invNr" column="invNr" type="text" nullable="true" />
        <field name="jahr" type="integer" nullable="true" />
        <field name="monat" type="integer" nullable="true" />
        <field name="tag" type="integer" nullable="true" />
        <field name="jh" type="integer" nullable="true" />
        <field name="erg" type="text" nullable="true" />
        <field name="jahrIi" column="jahrIi" type="integer" nullable="true" />
        <field name="monatIi" column="monatIi" type="integer" nullable="true" />
        <field name="tagIi" column="tagIi" type="integer" nullable="true" />
        <field name="jhIi" column="jhIi" type="integer" nullable="true" />
        <field name="ergIi" column="ergIi" type="text" nullable="true" />
        <field name="chronMinimum" column="chronMinimum" type="integer" nullable="true" />
        <field name="chronMaximum" column="chronMaximum" type="integer" nullable="true" />
        <field name="chronGlobal" column="chronGlobal" type="integer" nullable="true" />
        <field name="erwaehnteDaten" column="erwaehnteDaten" type="text" nullable="true" />
        <field name="unsicher" type="text" nullable="true" />
        <field name="datierung" type="text" nullable="true" />
        <field name="datierungIi" column="datierungIi" type="text" nullable="true" />
        <field name="anderePublikation" column="anderePublikation" type="text" nullable="true" />
        <field name="bl" type="text" nullable="true" />
        <field name="blOnline" column="blOnline" type="boolean" nullable="true" />
        <field name="uebersetzungen" type="text" nullable="true" />
        <field name="abbildung" type="text" nullable="true" />
        <field name="ort" type="text" nullable="true" />
        <field name="originaltitel" type="text" nullable="true" />
        <field name="originaltitelHtml" column="originaltitelHtml" type="text" nullable="true" />
        <field name="inhalt" type="text" nullable="true" />
        <field name="inhaltHtml" column="inhaltHtml" type="text" nullable="true" />
        <field name="bemerkungen" type="text" nullable="true" />
        <field name="daht" type="text" nullable="true" />
        <field name="ldab" type="text" nullable="true" />
        <field name="dfg" type="text" nullable="true" />
        <field name="ddbSer" column="ddbSer" type="text" nullable="true" />
        <field name="ddbVol" column="ddbVol" type="text" nullable="true" />
        <field name="ddbDoc" column="ddbDoc" type="text" nullable="true" />
        <field name="eingegebenAm" column="eingegebenAm" type="date" nullable="true" />
        <field name="zulGeaendertAm" column="zulGeaendertAm" type="date" nullable="true" />
        <field name="DatensatzNr" column="DatensatzNr" type="integer" nullable="true" />
        <field name="createdAt" column="createdAt" type="datetime" />
        <field name="modifiedAt" column="modifiedAt" type="datetime" />
        <one-to-many field="pictureLinks" target-entity="PictureLink" mapped-by="metadata" orphan-removal="true">
            <cascade>
                <cascade-persist/>
                <cascade-remove/>
            </cascade>
            <order-by>
                <order-by-field name="institution" direction="ASC" />
                <order-by-field name="name" direction="ASC" />
            </order-by>
        </one-to-many>
        <one-to-many field="mentionedDates" target-entity="MentionedDate" mapped-by="metadata">
            <order-by>
                <order-by-field name="jahr" direction="ASC" />
                <order-by-field name="monat" direction="ASC" />
                <order-by-field name="tag" direction="ASC" />
                <order-by-field name="jh" direction="ASC" />
            </order-by>
        </one-to-many>
    </entity>
</doctrine-mapping>