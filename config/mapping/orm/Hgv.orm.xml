        <field name="zusaetzlichSort" column="zusaetzlich_sort" type="text" nullable="true" />
        <field name="jahr" type="integer" nullable="true" />
        <field name="monat" type="integer" nullable="true" />
        <field name="tag" type="integer" nullable="true" />
        <field name="jh" type="integer" nullable="true" />
        <field name="erg" type="text" nullable="true" />
        <field name="jahrIi" column="jahr_ii" type="integer" nullable="true" />
        <field name="monatIi" column="monat_ii" type="integer" nullable="true" />
        <field name="tagIi" column="tag_ii" type="integer" nullable="true" />
        <field name="jhIi" column="jh_ii" type="integer" nullable="true" />
        <field name="ergIi" column="erg_ii" type="text" nullable="true" />
        <field name="chronMinimum" column="chron_minimum" type="integer" nullable="true" />
        <field name="chronMaximum" column="chron_maximum" type="integer" nullable="true" />
        <field name="chronGlobal" column="chron_global" type="integer" nullable="true" />
        <field name="erwaehnteDaten" column="erwaehnte_daten" type="text" nullable="true" />
        <field name="unsicher" type="text" nullable="true" />
        <field name="datierung" type="text" nullable="true" />
