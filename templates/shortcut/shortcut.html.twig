{% extends (layout is defined ? layout : 'base') ~ '.html.twig' %}

{% block title %}
  {% for hgv in data %}
    {{ hgv.publikationLang|raw ~ ' (TM/HGV ' ~ hgv.id ~ ')' ~ ' - ' ~ hgv.originaltitelHtml|raw }}
    {{ loop.last ? '' : ' - ' }}
  {% endfor %}
{% endblock %}

{% block body %}
  {% for hgv in data %}
      {% include 'browse/_datasheet.html.twig' with { 'record': hgv, 'translations': null } %}
  {% endfor %}
{% endblock %}

{% block javascripts %}
  {{ parent() }}
  <script src="{{ asset('js/datasheet.js') }}" type="text/javascript"></script>
{% endblock %}
